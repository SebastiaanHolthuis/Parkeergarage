<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?scenebuilder-stylesheet style.css?>

<BorderPane minHeight="800.0" minWidth="1200.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <left>

    </left>
    <center>
        <AnchorPane fx:id="carpark" />
    </center>
    <right>

    </right>
    <bottom>

    </bottom>
    <left>
        <TabPane prefHeight="620.0" prefWidth="351.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Statistics">
                    <content>
                        <AnchorPane fx:id="textstatistics" minHeight="0.0" minWidth="0.0" prefHeight="587.0" prefWidth="338.0">

                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Settings">
                    <content>
                        <AnchorPane fx:id="settings" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </left>
    <right>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Untitled Tab 1">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="445.0" prefWidth="232.0" />
                    </content>
                </Tab>
                <Tab text="Untitled Tab 2">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </right>
    <bottom>
        <BorderPane fx:id="bottomPane" prefHeight="180.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
            <right>
                <Pane fx:id="legend" prefHeight="122.0" prefWidth="326.0" BorderPane.alignment="CENTER" />
            </right>
            <left>
                <Pane prefHeight="194.0" prefWidth="210.0" style="-fx-border-style: solid; -fx-border-color: #ddd; -fx-border-width: 0 1px 0px 0px;" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="play" layoutX="37.0" layoutY="27.0" mnemonicParsing="false" text="Play" />
                        <Button fx:id="pause" layoutX="120.0" layoutY="27.0" mnemonicParsing="false" text="Pause" />
                        <Button fx:id="stepBack" layoutX="37.0" layoutY="69.0" mnemonicParsing="false" text="-10" />
                        <Button fx:id="stepForward" layoutX="131.0" layoutY="69.0" mnemonicParsing="false" text="+10" />
                        <Slider fx:id="tickPauseSlider" layoutX="37.0" layoutY="110.0" min="1.0" showTickLabels="true" showTickMarks="true" />
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </Pane>
            </left>
        </BorderPane>
    </bottom>
</BorderPane>
